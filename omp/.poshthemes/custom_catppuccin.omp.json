{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
	  "pink": "#FFAEE9",
    "lavender": "#BABBF1",
    "blue": "#8CAAEE",
    "orange": "#FFBB8F"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:blue",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "p:orange",
          "properties": {
            "folder_icon": "..üìÇ..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "<#ffffff>in</> {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:pink",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf594 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "go",
          "style": "powerline",
          "parse_mod_file": "true",
          "foreground": "#7FD5EA",
          "template": " [üêπ {{ .Major }}.{{ .Minor }}] "
        },
        {
          "type": "python",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "lightGreen"
        },
        {
          "type": "rust",
          "style": "powerline",
          "foreground": "#f55200",
          "template": " [ü¶Ä {{ .Major }}.{{ .Minor }}] "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground_templates": ["{{ if gt .Code 0 }}#ff5959{{else}}p:lavender{{ end }}"],
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}